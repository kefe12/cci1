    <Button fx:id="btn" onAction="#handleButtonAction" text="insert!" GridPane.rowIndex="2" />
    <Button fx:id="btn1" onAction="#selectButtonAction" text="select all!" GridPane.columnIndex="6" GridPane.rowIndex="2" />
    <Button fx:id="btn3" onAction="#selectDistinictButtonHandle" text="select distinict!" GridPane.columnIndex="3" GridPane.rowIndex="2" />
    <Button fx:id="btn4" onAction="#selectrowButtonHandle" text="select row!" GridPane.columnIndex="2" GridPane.rowIndex="2" />
    <Button fx:id="btn5" onAction="#updateButtonHandle" text="update" GridPane.columnIndex="5" GridPane.rowIndex="2" />
    <TableView fx:id="tbl" GridPane.columnIndex="1" GridPane.columnSpan="10" GridPane.hgrow="ALWAYS" GridPane.rowIndex="5" GridPane.vgrow="ALWAYS" />
