<GridPane fx:id="gp" alignment="TOP_LEFT" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.cci1.HelloController">

    <Label text="Hello, World!" GridPane.rowIndex="0" />
    <TextField fx:id="txt1" maxHeight="20" promptText="SSID" GridPane.columnIndex="1" />

    <TextField fx:id="txt2" maxHeight="20" promptText="Studid" GridPane.columnIndex="2" />
